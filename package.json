  "scripts": {
    "build": "vite build",
    "build:ssr": "vite build && vite build --ssr",
    "dev": "concurrently \"npm run frontend\" \"npm run backend-dev\" \"npm run reverb\" \"npm run queue\" \"npm run schedule\"",
    "dev-test": "concurrently \"npm run frontend\" \"npm run backend-test\" \"npm run reverb\" \"npm run queue\" \"npm run schedule\"",
    "format": "prettier --write resources/",
    "format:check": "prettier --check resources/",
    "lint": "eslint . --fix",
    "types": "tsc --noEmit",
    "frontend": "vite",
    "backend-dev": "php artisan serve",
    "backend-test": "php artisan serve --env=testing",
    "reverb": "php artisan reverb:start",
    "queue": "php artisan queue:work",
    "schedule": "php artisan schedule:work"
